{
    "name": "chessboard",
    "version": "1.0.0",
    "description": "Chess UI written in Typescript",
    "type": "module",
    "module": "chessboard.js",
    "typings": "chessboard.d.ts",
    "dependencies": {},
    "devDependencies": {
      "@rollup/plugin-typescript": "^8",
      "@typescript-eslint/eslint-plugin": "^5",
      "@typescript-eslint/parser": "^5",
      "eslint": "^8",
      "prettier": "^2",
      "rollup": "^2",
      "rollup-plugin-terser": "^7",
      "tslib": "^2",
      "typescript": "^4"
    },
    "scripts": {
      "prepare": "$npm_execpath run compile",
      "compile": "tsc --outDir . --sourceMap --declaration",
      "lint": "eslint src/*.ts",
      "format": "prettier --write .",
      "check-format": "prettier --check .",
      "dist": "rollup --config"
    },
    "files": [
      "/*.js",
      "/*.d.ts",
      "/*.js.map",
      "/assets/*.css",
      "/src/*.ts",
      "!/rollup.config.js"
    ],
    "repository": "https://github.com/Harry-Hopkinson/chessboard-typescript",
    "keywords": [
      "chess",
      "chessboard",
      "ui",
      "typescript"
    ],
    "author": "Harry Hopkinson (https://github.com/ornicar)",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/Harry-Hopkinson/chessboard-typescript/issues"
    }
  }